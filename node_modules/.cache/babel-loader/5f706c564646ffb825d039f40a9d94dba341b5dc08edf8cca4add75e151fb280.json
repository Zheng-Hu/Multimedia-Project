{"ast":null,"code":"import axios from 'axios';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      tunnelCarSpeedList: null\n    };\n  },\n  mounted() {\n    this.drwaParkingBarChart();\n  },\n  methods: {\n    async drwaParkingBarChart() {\n      // let response = await axios.get('https://api.data.gov.hk/v1/carpark-info-vacancy?data=vacancy')\n      // let response1 = await axios.get('https://api.data.gov.hk/v1/carpark-info-vacancy?lang=zh_TW')\n      // console.log(response)\n      // console.log(response1)\n      let response = await axios.get('https://dashboard.data.gov.hk/api/traffic-speed?format=json');\n      console.log(response.data);\n      this.tunnelCarSpeedList = response.data.map(function (item) {\n        return item.traffic_speed;\n      });\n      console.log(this.tunnelCarSpeedList);\n      var laborforceLineChart = echarts.init(document.getElementById(\"parkingBarChart\"));\n      var option;\n      option = {\n        xAxis: {\n          type: 'category',\n          data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: [120, 200, 150, 80, 70, 110, 130],\n          type: 'bar'\n        }]\n      };\n      laborforceLineChart.setOption(option);\n    }\n  }\n};","map":{"version":3,"mappings":"AAsEA;AACA;AACA;EACAA;EACAC;IACA;MACAC;IACA;EACA;EAEAC;IACA;EACA;EAEAC;IACA;MACA;MACA;MACA;MACA;MACA;MACAC;MACA;QACA;MACA;MACAA;MACA;MACA;MAEAC;QACAC;UACAC;UACAP;QACA;QACAQ;UACAD;QACA;QACAE,SACA;UACAT;UACAO;QACA;MAEA;MACAG;IACA;EACA;AACA","names":["name","data","tunnelCarSpeedList","mounted","methods","console","option","xAxis","type","yAxis","series","laborforceLineChart"],"sourceRoot":"src/views","sources":["HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div id=\"parkingBarChart\" style=\"height: 500px; width:500px\"></div>\n    <div  class=\"tunnel-traffic\">\n      <div class=\"block1\" style=\"margin-right: 30px;\">\n        <div class=\"header\">\n          <span>西区海底隧道</span>\n        </div>\n        <div class=\"content\">\n          <div>\n            <h6>九龙</h6>\n            <div class=\"speed-content\">\n              <div class=\"speed-left\">{{this.tunnelCarSpeedList[1]}}</div>\n              <img style=\"margin-right: 10px;\" width=\"30\" src=\"../assets/traffic-icon/greenup.png\">\n              <img width=\"30\" src=\"../assets/traffic-icon/greendown.png\">\n              <div class=\"speed-right\">{{this.tunnelCarSpeedList[5]}}</div>\n            </div>\n              <h6>香港</h6>\n          \n          </div>\n        </div>\n\n      </div>\n      <div class=\"block1\" style=\"margin-right: 30px;\">\n        <div class=\"header\">\n          <span>红磡海底隧道</span>\n        </div>\n        <div class=\"content\">\n          <div>\n            <h6>九龙</h6>\n            <div class=\"speed-content\">\n              <div class=\"speed-left\">{{this.tunnelCarSpeedList[4]}}</div>\n              <img style=\"margin-right: 10px;\" width=\"30\" src=\"../assets/traffic-icon/greenup.png\">\n              <img width=\"30\" src=\"../assets/traffic-icon/greendown.png\">\n              <div class=\"speed-right\">{{this.tunnelCarSpeedList[0]}}</div>\n            </div>\n              <h6>香港</h6>\n          \n          </div>\n        </div>\n\n      </div>\n      <div class=\"block1\">\n        <div class=\"header\">\n          <span>东区海底隧道</span>\n        </div>\n        <div class=\"content\">\n          <div>\n            <h6>九龙</h6>\n            <div class=\"speed-content\">\n              <div class=\"speed-left\">{{this.tunnelCarSpeedList[1]}}</div>\n              <img style=\"margin-right: 10px;\" width=\"30\" src=\"../assets/traffic-icon/greenup.png\">\n              <img width=\"30\" src=\"../assets/traffic-icon/greendown.png\">\n              <div class=\"speed-right\">{{this.tunnelCarSpeedList[1]}}</div>\n            </div>\n              <h6>香港</h6>\n          \n          </div>\n        </div>\n\n      </div>\n    </div>\n\n\n  </div>\n</template>\n\n<script>\n\n\nimport axios from 'axios';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n       tunnelCarSpeedList: null\n    }\n  },\n\n  mounted() {\n    this.drwaParkingBarChart();\n  },\n\n  methods: {\n    async drwaParkingBarChart() {\n      // let response = await axios.get('https://api.data.gov.hk/v1/carpark-info-vacancy?data=vacancy')\n      // let response1 = await axios.get('https://api.data.gov.hk/v1/carpark-info-vacancy?lang=zh_TW')\n      // console.log(response)\n      // console.log(response1)\n      let response = await axios.get('https://dashboard.data.gov.hk/api/traffic-speed?format=json')\n      console.log(response.data)\n      this.tunnelCarSpeedList = response.data.map(function (item) {\n        return item.traffic_speed\n      })\n      console.log(this.tunnelCarSpeedList)\n      var laborforceLineChart = echarts.init(document.getElementById(\"parkingBarChart\"));\n      var option;\n\n      option = {\n        xAxis: {\n          type: 'category',\n          data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [\n          {\n            data: [120, 200, 150, 80, 70, 110, 130],\n            type: 'bar'\n          }\n        ]\n      };\n      laborforceLineChart.setOption(option);\n    }\n  }\n}\n</script>\n\n<style scoped>\n.tunnel-traffic {\n  display: flex;\n  flex-direction: row;\n}\n.speed-content {\n  display: inline-flex;\n  flex-direction: row;\n  align-items: center;\n}\n.speed-left {\n  margin-right: 20px;\n}\n\n.speed-right {\n  margin-left: 20px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}